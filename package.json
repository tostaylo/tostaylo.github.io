{
  "name": "rust-fel-portfolio",
  "version": "1.0.0",
  "description": "A portfolio site built with RustFEL",
  "main": "index.js",
  "scripts": {
    "start": "tsc --watch",
    "build": "npm run build-ts && npm run convert && sh build.sh && npm run node-server",
    "convert": "cd converter && node index.js",
    "build-ts": "tsc",
    "cypress:open": "cypress open",
    "cypress:run": "cypress run --browser chrome --headless --config video=false,screenshotOnRunFailure=false",
    "node-server": "http-server --port 8000"
  },
  "author": "Torre Taylor",
  "license": "ISC",
  "devDependencies": {
    "cypress": "^4.12.1",
    "http-server": "^0.12.3",
    "typescript": "^4.1.3"
  }
}
